    @* META - PÁGINA *@
    <meta charset="UTF-8">
    <meta name=viewport content="width=device-width, initial-scale=1" />
    <meta name="description" content="código cores cabos metálicos" />
    <meta name="keywords" content="código cores cabos metálicos" />
    @* GOOGLE WEBMASTERS *@
    <meta name="google-site-verification" content="4pvD-0unbLZfJMO9XFdivBT5du6qub36284VaTxFqSs" />
    @* LINKS *@
    <link href="~/Imagens/Badisco-Online.ico" rel="shortcut icon" type="image/x-icon">
    <link href="https://fonts.googleapis.com/css?family=Open+Sans" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Fredoka+One" rel="stylesheet">
