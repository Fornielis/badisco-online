                </ul>
            </div>
        </div>
    </div>
    @* BODY *@
    @RenderBody()
    @* RODAPÉ *@
    @Html.Partial("~/Views/LayoutPadrao/_Rodape.cshtml")
    @* PROGRESS *@
    <div id="progress">
        <div><img src="~/Imagens/LoaderAzul.gif" alt="IMG CARREGAMENTO" /></div>
    </div>
    @* PAINEL - RESPOSTA *@
    <div id="box-resposta"></div>
</body>
</html>
