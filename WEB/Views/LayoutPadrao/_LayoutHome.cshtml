        (adsbygoogle = window.adsbygoogle || []).push({
            google_ad_client: "ca-pub-5168986806799330",
            enable_page_level_ads: true
        });
    </script>
    @* GOOGLE ANALITICS *@
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-120978524-1"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag() { dataLayer.push(arguments); }
